package Threads;


	public class FoodOrder extends Thread {

	    String task;
	    FoodOrder(String task) {
	        this.task = task;
	    }

	    public void run() {
	        System.out.println(task+" started");
	    }
	    public static void main(String[] args) {
	        FoodOrder cooking = new FoodOrder("Cooking");
	        FoodOrder packing = new FoodOrder("Packing");
	        FoodOrder serving = new FoodOrder("Serving");

	        cooking.start();
	        packing.start();
	        serving.start();
	    }
	}

	/*
	â„¹ï¸ Ponints to remember
	ğŸ’« Extend Thread
	ğŸ’« Override run()
	ğŸ’« Use start()
	ğŸ’« It is less flexible because of inheritance limitati
	*/


