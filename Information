package Aggression;

public class Information {
	
	    char blockNo;
	    int floorNo, roomNo, bedNo;

	    public Information(char blockNo, int floorNo,int roomNo, int bedNo) {
	        this.blockNo = blockNo;
	        this.floorNo = floorNo;
	        this.roomNo = roomNo;
	        this.bedNo = bedNo;
	    }
	}

