package composition;
class Engine {
	
    void start() {
        System.out.println("Engine starts");
    }
}
public class Car {
    private Engine engine;
    Car() {
        // Engine class object will be created here
        engine = new Engine();
    }   
    
    void drive() {
        engine.start();
        System.out.println("Car is Moving");
    }
    public static void main(String[] args) {
        Car car = new Car();
        car.drive();
    }
}
